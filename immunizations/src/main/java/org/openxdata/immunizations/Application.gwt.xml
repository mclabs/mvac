<!DOCTYPE module PUBLIC "//gwt-module/" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.2/distro-source/core/src/gwt-module.dtd">
<module >

      <!-- Inherit the core Web Toolkit stuff.                        -->
      <inherits name='com.google.gwt.user.User'/>

      <!--  inherit css based theme -->
      <inherits name='com.google.gwt.user.theme.standard.Standard'/>


	<inherits name='net.sf.gilead.Gilead4Gwt' />
	<inherits name='org.openxdata.server.admin.Model' />

	<inherits name='org.purc.purcforms.SharedLib' />
	<inherits name='com.google.gwt.xml.XML' />
	<inherits name='org.zenika.widget.DatePickerModule' />
	<inherits name='com.allen_sauer.gwt.dnd.gwt-dnd' />
        
      <!-- Specify the app entry point class.                         -->
      <entry-point class='org.openxdata.immunizations.client.Application'/>

      <!-- Specify the application specific style sheet.              -->
      <stylesheet src='Application.css' />

      <set-property name="locale" value="default" />
    <set-property name="user.agent" value="gecko1_8" />
    <extend-property name="locale" values="en" />
    <set-property name="locale" value="en" />

</module>
