package org.openxdata.server.admin.model.exception;

public class OpenXDataValidationException extends OpenXDataException {

	private static final long serialVersionUID = -7688826023254204273L;

	public OpenXDataValidationException() {
		super();
	}

	public OpenXDataValidationException(String message, Throwable cause) {
		super(message, cause);
	}

	public OpenXDataValidationException(String message) {
		super(message);
	}

	public OpenXDataValidationException(Throwable throwable) {
		super(throwable);
	}

}
